{
  "version": 2,
  "builds": [
    {
      "src": "front/dist/**",
      "use": "@vercel/static"
    },
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/index.js"
    },
    {
      "src": "/(.*)", 
      "dest": "/front/dist/index.html" 
    }
  ],
  "functions": {
    "src/index.js": {
      "includeFiles": "front/dist/index.html"
    }
  }
}
